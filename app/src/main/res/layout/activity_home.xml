<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homeq"
    tools:context=".Home">

  <Button
      android:id="@+id/add_button6"
      android:layout_width="200dp"
      android:layout_height="32dp"
      android:layout_marginEnd="108dp"
      android:layout_marginRight="108dp"
      android:layout_marginBottom="232dp"
      android:background="@drawable/rw"
      android:clickable="true"

      android:focusable="true"
      android:hint="Prescription"
      android:textColor="#C54E4E"
      android:textColorHint="#150440"
      android:textColorLink="#9A7D35"
      android:textSize="16sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:srcCompat="@drawable/ic_add" />

  <Button
      android:id="@+id/add_button3"
      android:layout_width="232dp"
      android:layout_height="61dp"
      android:layout_marginEnd="76dp"
      android:layout_marginRight="76dp"
      android:layout_marginBottom="36dp"
      android:textStyle="bold"
      android:background="@drawable/rw"
      android:clickable="true"
      android:focusable="true"
      android:hint="Delivery Charge"
      android:textColorHint="#030A37"
      android:textSize="16sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:srcCompat="@drawable/ic_add" />

</androidx.constraintlayout.widget.ConstraintLayout>